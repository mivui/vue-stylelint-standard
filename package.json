{
  "name": "vue-stylelint-standard",
  "version": "16.3.0",
  "description": "quickly start stylelint in vue",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "eslint": "eslint --fix **/*.{ts,js}",
    "test": "vitest"
  },
  "dependencies": {
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.4",
    "rollup": "^4.21.2",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint-standard": "^9.3.0",
    "vitest": "^2.0.5"
  },
  "author": "mivui",
  "license": "MIT",
  "keywords": [
    "vue",
    "stylelint",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mivui/vue-stylelint-standard.git"
  },
  "bugs": {
    "url": "https://github.com/mivui/vue-stylelint-standard/issues"
  },
  "homepage": "https://github.com/mivui/vue-stylelint-standard#readme"
}
